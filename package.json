{
  "name": "portfolio-website-m-labedzki",
  "version": "1.2.3",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "coverageReport": "yarn test --changedSince master --coverage --coverageReporters=html",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "updateVersion1Push": "yarn version --major && git push --follow-tags",
    "updateVersion2Push": "yarn version --minor && git push --follow-tags",
    "updateVersion3Push": "yarn version --patch && git push --follow-tags",
    "updateVersion1": "yarn version --major",
    "updateVersion2": "yarn version --minor",
    "updateVersion3": "yarn version --patch",
    "storybook": "storybook dev -p 6006",
    "storybookDebug": "storybook dev -p 6006 --loglevel=debug",
    "build-storybook": "storybook build",
    "translate:parse": "i18next 'src/**/*.*s*' -c i18next-parser.config.mjs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/index.js",
      "src/reportWebVitals.js",
      "src/stories",
      "src/data",
      "src/images",
      "src/router",
      "src/i18n-always-include.js",
      "src/i18n.js",
      "src/lib/testHelpers.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-dnd|dnd-core|@react-dnd|dnd-multi-backend|.*\\.css)"
    ]
  },
  "resolutions": {
    "tailwindcss": "3.2.0",
    "react-error-overlay": "^6.1.0",
    "browserslist": "^4.25.0"
  },
